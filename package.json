    "relative-to-date": "0.0.2"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.7.5",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
